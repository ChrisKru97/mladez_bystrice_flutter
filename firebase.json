{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
			{
				"source": "/cv",
				"destination": "/cv.html"
			},
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
